    .navbar-default .navbar-toggle:focus {
        background: #000;
    }
    .navbar-collapse {
        padding: 0px;
    }
    .caption_img {
        display: none;
    }
    .caption {
        margin: 66px 0 26px;
    }
    .caption h2 {
