
body{
    color: #fff;
    background: #fff;
}
h1{
    font-family: Rockwell, "Courier New", Courier, Georgia,
    Times, "Times New Roman", serif;
    font-weight: bold;
    color: #fff;
}
a,
a:hover{ color: #a8d039; }
.thumbnail{
    border-radius: 0;
    padding: 0;
    min-height: 340px;
    background: #fff;
    position: relative;

}
.logo img{
    float: left;
}
.thumbnail .holder{
    height: 200px;
    width: 200px;
    margin: 0 auto;
}
.cancle_icon{
    position: absolute;
    right: 10px;
    top: 9px;
}
.cancle_icon .fa{
    color: #676767;
    font-size: 28px;
}
.thumbnail .caption{
    margin: 0;
}
.thumbnail .caption a{
    color: #696969;
}
.hidden_p{
    display: none;
}
.active_p{
    color: #a8d039 !important;
    font-weight: bold;
}
.thumbnail .caption .list-inline>li:last-child{
    border:0;
}
.thumbnail .caption .list-inline>li{
    border-right: 1px solid  #484848;
    line-height: 12px;
}

.thumbnail p{
    color: #676767;
}
.thumbnail img{
    width: 100%;
}
.thumbnail h3{
    color: #a8d039;
}
.thumbnail .breadcrumb{
    padding: 0;
    background: transparent;
}
.main-area .nav {
  padding: 15px 0;
}
.main-area .nav.nav-tabs{
    padding: 0;
}
.main-area .nav > li > a{
    border-radius: 0;
    margin: 0 20px;
    font-size: 16px;
    color: #696969;
}
.main-area .nav > li > a h4{
    color: #A8D039;
    border-right: 1px solid;
    padding: 0 31px 0 0;
}
.main-area .nav > li > a:hover,
.main-area .nav > li > a:focus {
    background-color: transparent;
    text-decoration: none;
}
.btn-green{
  color: #000;
  background-color: #a8d039;
  border-color: #a8d039;
}
.btn-green:hover,
.btn-green:focus,
.btn-green.focus,
.btn-green:active,
.btn-green.active{
  color: #000;
  background-color: #a8d039;
  border-color: #a8d039;
}

#wrapper {
    padding-left: 0;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled {
    padding-left: 250px;
}

#sidebar-wrapper {
    z-index: 1000;
    position: fixed;
    left: 250px;
    width: 0;
    height: 100%;
    margin-left: -250px;
    overflow-y: auto;
    background: #484848;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled #sidebar-wrapper {
    width: 250px;
}

#page-content-wrapper {
    width: 100%;
    position: absolute;
    padding: 0;
}

#wrapper.toggled #page-content-wrapper {
    position: absolute;
    margin-right: -250px;
}

/* Sidebar Styles */

.sidebar-nav {
    position: absolute;
    top: 0;
    width: 250px;
    margin: 0;
    padding: 0;
    list-style: none;
}

.sidebar-nav li {
    text-indent: 20px;
}

.sidebar-nav li a {
    display: inline-block;
    text-decoration: none;
    color: #fff;
    margin: 10px 0 0;
}

.sidebar-nav li a:hover {
    text-decoration: none;
    color: #a8d039;
}
.sidebar-nav strong{
 color: #a8d039;
}
.sidebar-nav li ul{
    list-style: none;
    color: #fff;
}

.sidebar-nav li a:active,
.sidebar-nav li a:focus {
    text-decoration: none;
}

.sidebar-nav > .sidebar-brand {
    padding: 70px 25px;
    font-size: 18px;
    line-height: 60px;
}

.sidebar-nav > .sidebar-brand a {
    color: #fff;
}

.sidebar-nav > .sidebar-brand a:hover {
    color: #fff;
    background: none;
}
.sidebar-nav li.profile-area a{
    display: inline;
}
.profile-area{
    background: #686867;
    overflow: hidden;
    margin: 0 0 25px;
}
.profile-area .image-holder {
    border-radius: 100%;
    float: left;
    height: 70px;
    margin: 5px 0 0 10px;
    overflow: hidden;
    width: 70px;
}
.profile-area .user_name{
    overflow: hidden;
    padding: 14px 0 5px;
    font-size: 16px;
}
.profile-area .user_name a{
    font-weight: bold;
    color: #a8d039;
    display: inline;
    padding: 14px 0 0;
}
.profile-area span{
    overflow: hidden;
    padding: 0 0 4px;
    float: left;
}
.sidebar-nav .fa{
 margin: 0 5px 0 0;
}
.profile-area .fa{
    display: inline;
    margin: 0 5px 0 0;
}
.my_Parties{
  padding: 0 0 0 15%;
}
.my_Parties li{
    padding: 0 0 0 11%;
    margin: 0;
}
.my_Parties a{
    margin: 0;
}
.sidebar-nav .btn-green{
    margin: 22%;
}
.banner{
    background: #a8d039;
    padding: 30px 30px 0;
}
.banner .caption{
    margin: 35px 0 0;
}
.banner .caption h1{
    font-size: 47px;
    margin: 28px 0;
}
.banner .caption p{
    font-size: 16px;
    line-height: 30px;
    margin: 0 0 9px;
}
.banner .caption .btn-primary {
    margin-top: 25px;
}
.left-inner-addon {
    position: relative;
}
.left-inner-addon input {
    padding-left: 30px;    
}
.left-inner-addon i {
    position: absolute;
    padding: 10px 12px;
    pointer-events: none;
}

.right-inner-addon {
    position: relative;
}
.right-inner-addon input {
    padding-right: 30px;    
}
.right-inner-addon i {
    position: absolute;
    right: 20%;
    padding: 10px 12px;
    pointer-events: none;
    font-size: 25px;
    cursor: pointer;
}
.form-control{
    border-radius: 0;
    height: 45px;
}
.btn-gray {
   height: 45px;
   border-radius: 0;
   background: #686867;
   border-color: #686867;
   color: #fff;
   font-size: 14px;
   padding: 8px 25px;
}
.btn-gray:hover,
.btn-gray:focus,
.btn-gray.focus,
.btn-gray:active,
.btn-gray.active,
.open>.dropdown-toggle.btn-gray {
  background: #686867;
  border-color: #686867;
  color: #fff;
}
.radio_search.dropdown-menu{
    left: -162px ;
    width: 262px;
    padding: 0 0 15px;
    border:1px solid #a8d039;
    text-align: center;
    
}
.radio_search.dropdown-menu label{
    margin: 0 8px 0 0;
}
.dropdown-menu{
    left: -420px ;
    border-radius: 0;

}
.open > .dropdown-menu{
 background: #686867;
}
.btn .caret{
    color: #a8d039;
    margin: 0 5px;
}

.form-control {
  float: left;
  width: 80%;
}

.btn-group{
  float: left;
  margin: 0 0 0 10px;
}
.btn-primary {
  color: #fff;
  background-color: #337ab7;
  border-color: #2e6da4;
}
.btn-primary.active, .btn-primary.focus, .btn-primary:active, .btn-primary:focus, .btn-primary:hover, .open>.dropdown-toggle.btn-primary {
  color: #fff;
  background-color: #286090;
  border-color: #204d74;
}
.mobile_header{
    display: none;
    overflow: hidden;
}
.input-area .fa {
    color: #686867;
}
.form-control:focus {
    border-color: #a8d039;
}
.mobile_header{
    display: none;
    overflow: hidden;
    margin: 0 0 15px;
}
.slide-box{
    padding: 10px;
    margin-top: 15px;
}
.slide-box h4{
    color: #fff;
    margin:15px 0;
}
.slide-box li{
  overflow: hidden;
  padding: 10px 0;

}
.slider .tooltip-inner{
    display: none;
}
.slider .tooltip.top,
.slider .tooltip-arrow{
    display: none;
}
.slide-box label{
    margin: 0 10px;
}
.mt15{
    margin: 15px 0 0;
}
/*sport_pages start*/
.sport_header{
    background: url(<%= asset_path 'vendor/Sports_banner.jpg' %>) no-repeat center center;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
}
.sport_header .sport_caption {
  padding: 133px 0;
}
.sport_header .sport_caption h2{
    font-family: 'Fjalla One', Helvetica, Arial, sans-serif;
    text-transform: uppercase;
    color: #484848;
    font-size: 30px;
    font-weight: bold;
}
.ptb15{
    padding-bottom:15px;
    padding-top: 15px;
}
.main-area .panel-group .panel{
    border-radius: 0;
}
.main-area .panel-default>.panel-heading{
    overflow: hidden;
    background-color: #686867;
    border-bottom: 1px solid #fff;
    border-radius: 0;
}
.main-area .panel-default>.panel-heading h3{
    color: #fff;
    margin: 30px 0 0;
}
.main-area .panel{
    border:0;
}
.main-area .panel-heading {
  padding: 22px 15px;
}
.main-area .panel-group {
  margin-bottom: 18px;
}
.main-area .panel-group .panel+.panel{
    margin-top: 0;
}
.main-area .list-group li{

    background-color: #686867;
    border-bottom: 1px solid #fff;
    border-radius: 0;
    font-size: 16px;
    padding: 20px 0;
}
.main-area .list-group h4{
    color: #fff;
    font-weight: bold;
    margin: 28px 0 0;
}
.main-area .list-group .list-group-item{
    border: 0;
}
.main-area .list-group .list-group-item .btn{
    padding: 8px 25%;
    color: #696969;
    
}

.mtb25{
    margin: 26px 0;
}
.panel-heading .image-holder{
    float: left;
    margin: 5px 0 0 10px;
    border-radius: 100%;
    width: 80px;
    height: 80px;
    overflow: hidden;
}
.main-area .list-group .list-group-item .image-holder {
  float: left;
  margin: 5px 0 0 10px;
  border-radius: 100%;
  width: 109px;
  height: 109px;
  overflow: hidden;
}
.main-area .list-group .list-group-item .image-holder img {
    height: 100%;
}
.panel-body h4{
    float: left;
}
.panel-body .star{
    float: right;
    margin: 10px 0 0;
}
.panel-body .star i{
    color: #a8d039;
    cursor: pointer;
}
/*sport_pages end*/
@media(max-width:1200px) {
   .banner .caption h1 {
    font-size: 43px;
    margin: 14px 0px;
}
}
@media(max-width:900px) {
   .banner .caption h1 {
    font-size: 43px;
    margin: 14px 0px;
}
.box-hide{
    display: none;
}
.banner .caption .btn-primary {
    margin: 25px 0;
}
.right-inner-addon i{
    right: 30%;
}
.banner .form-control {
    width: 70%;
}
}
@media(max-width:768px) {

    .mobile_header{
        display: block;
        overflow: hidden;
        width: 68%;
    }
    .mobile_header .btn-default{
        margin: 15px 0;
    }

    .right-inner-addon i{
        right: 40%;
    }
    .banner .form-control {
        width: 60%;
    }
    .btn-group{
        margin: 0;
    }
    .banner .caption h1 {
        font-size: 36px;
        margin: 14px 0px;
    }
    .slider.slider-horizontal {
        width: 150px !important;
    }
    .dropdown-menu.pull-right {
        right: 0px !important;
        left: auto;
    }
}
@media(min-width:768px) {

    #wrapper {
        padding-left: 250px;
    }

    #wrapper.toggled {
        padding-left: 0;
    }

    #sidebar-wrapper {
        width: 250px;
    }

    #wrapper.toggled #sidebar-wrapper {
        width: 0;
    }

    #page-content-wrapper {
        padding: 0;
        position: relative;
    }

    #wrapper.toggled #page-content-wrapper {
        position: relative;
        margin-right: 0;
    }
}