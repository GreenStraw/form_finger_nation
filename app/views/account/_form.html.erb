<%#= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :class => "form" }) do |f| %>
<%= form_for @user, :url => create_account_path, html: { class: 'form login' } do |f| %>
 <%#= devise_error_messages! %>
 
 <%= fields_for(:tenant) do |ff| %>
 <div class="input-group">
   <span class="input-group-addon">O</span>
   <%= ff.text_field :name, :class => "form-control", :placeholder =>'Organization' %>
 </div>
 </br>
 <% end %>
 
 <div class="input-group">
   <span class="input-group-addon">E</span>
   <%= f.text_field :email, :class => "form-control", :placeholder =>'Email Address' %>
 </div>
 </br>

 <div class="input-group">
   <span class="input-group-addon">P</span>
   <%= f.password_field :password, :class => "form-control", :placeholder =>'Password' %>
 </div>
 </br>

<div class="input-group">
  <span class="input-group-addon">P</span>
  <%= f.password_field :password_confirmation, :class => "form-control", :placeholder =>'Password confirmation' %>
</div>
</br>

<div class="input-group">
  <span class="input-group-addon">U</span>
  <%= f.text_field :username, :class => "form-control", :placeholder =>'Username' %>
</div>
</br>

<div class="input-group">
  <button class="btn btn-primary" type="submit">Sign up</button>
</div>

<% end %>
</br>