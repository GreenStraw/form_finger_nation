<div id="wrapper">
  <%= render "layouts/black_side" %>
  <div id="page-content-wrapper">
    <div class="container-fluid">
      <div class="Following_header sport_header  row">
        <div class="col-md-12">
          <div class="mobile_header  ">
            <a href="#menu-toggle" class="btn btn-default pull-left " id="menu-toggle"><i class="fa fa-bars"></i> </a>
            <div class="logo pull-left">
              <%= link_to(image_tag("vendor/logo2.png"), root_path, alt: "FFN") %>
            </div>
          </div>
          <div class="sport_caption col-md-4 pull-right ">
            <h2 class="form-title">Jobs</h2>
          </div>
        </div>
      </div>
      <div class="main-area">
        <div class="row">
          <div class="follow">
            <div class="col-md-6">
              <h3 class="form-title">Jobs</h3>
              <p>
                There are no open positions at this time, but we're always looking to recruit talented people. Please send your resume to info@foamfingernation.com if you're interested in joining us.
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

