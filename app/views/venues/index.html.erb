<div class='container'>
  <table class='table table-hover borderless'>
    <tbody>
      <% @venues.each do |venue| %>
        <tr>
          <td><%= image_tag(venue.image_url, size: '80') unless venue.image_url.blank? %></td>
          <td><h4><%= link_to(venue.name, venue) %></h4></td>
          <td><%= venue.address.city_state %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
